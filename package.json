{
  "name": "@acl/ui",
  "version": "1.5.8",
  "description": "UST Healthproof RAQ Component Library",
  "author": "Jaison Jeyakrishnan",
  "license": "UST Healthproof",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "repository": {
    "url": "https://github.com/Advantasure-RAQ/rqsr-ui-design-library"
  },
  "scripts": {
    "playground": "webpack server --config webpack.config.ts",
    "storybook": "storybook dev -p 2000",
    "build:storybook": "storybook build",
    "test": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "rimraf dist/ && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --project tsconfig.build.json && npm run copystatic:esm",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs --project tsconfig.build.json && npm run copystatic:cjs",
    "copystatic:esm": "copyfiles -V -u 1 -e \"./src/development/**/*\" \"./src/**/*.{css,svg,png,ttf,woff,woff2}\" ./dist/esm",
    "copystatic:cjs": "copyfiles -V -u 1 -e \"./src/development/**/*\" \"./src/**/*.{css,svg,png,ttf,woff,woff2}\" ./dist/cjs",
    "pack:release": "npm run clean:tgz && npm pack --pack-destination ./release",
    "clean:tgz": "node -e \"require('rimraf').sync(`./release/acl-ui-${require('./package.json').version}.tgz`)\"",
    "lint": "eslint --config .eslintrc.js \"src/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --config .prettierrc.js --write \"src/**/*.{js,jsx,ts,tsx,css}\"",
    "clean:node_modules": "rimraf node_modules/",
    "release": "npm install && tsc --noEmit && npm run format && npm run lint && npm run build && npm run pack:release"
  },
  "keywords": [
    "acl",
    "react",
    "toast",
    "components"
  ],
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.2",
    "@mui/material": "^6.4.2",
    "@mui/x-date-pickers": "^7.25.0",
    "date-fns": "^4.1.0",
    "notistack": "^3.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.5",
    "react-virtuoso": "^4.12.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.7",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@babel/register": "^7.25.9",
    "@chromatic-com/storybook": "^3.2.4",
    "@storybook/addon-essentials": "^8.6.0-alpha.0",
    "@storybook/addon-interactions": "^8.6.0-alpha.0",
    "@storybook/addon-webpack5-compiler-swc": "^2.0.0",
    "@storybook/blocks": "^8.6.0-alpha.0",
    "@storybook/react": "^8.6.0-alpha.0",
    "@storybook/react-webpack5": "^8.6.0-alpha.0",
    "@storybook/test": "^8.6.0-alpha.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.2.1",
    "copy-webpack-plugin": "^12.0.2",
    "copyfiles": "^2.4.1",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.0",
    "eslint": "^8.57.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-storybook": "^0.11.2",
    "eslint-plugin-unused-imports": "^4.1.4",
    "eslint-webpack-plugin": "^4.2.0",
    "favicons-webpack-plugin": "^6.0.1",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "html-webpack-plugin": "^5.6.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-runner-eslint": "^2.2.1",
    "mini-css-extract-plugin": "^2.9.2",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "sass-loader": "^16.0.4",
    "storybook": "^8.6.0-alpha.0",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  }
}
